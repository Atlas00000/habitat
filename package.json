{
  "name": "arctic-region-viewer",
  "version": "1.0.0",
  "description": "Standalone Arctic Region 3D viewer component with polar bear and arctic environment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "next dev --port 3000",
    "dev:fast": "next dev --turbo --port 3000 --experimental-turbo",
    "dev:refresh": "next dev --turbo --port 3000 --experimental-turbo --experimental-https",
    "dev:debug": "next dev --turbo --port 3000 --inspect",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && next dev --turbo --port 3000",
    "dev:reset": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .turbo && next dev --turbo --port 3000",
    "dev:setup": "node scripts/dev-setup.js",
    "prepare": "npm run build",
    "start": "next dev",
    "build-app": "next build",
    "serve": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "rm -rf .next && rm -rf node_modules/.cache && rm -rf dist && rm -rf .turbo",
    "reset": "rm -rf .next && rm -rf node_modules/.cache && npm run dev",
    "kill-port": "lsof -ti:3000 | xargs kill -9"
  },
  "keywords": [
    "react",
    "three.js",
    "3d",
    "arctic",
    "polar-bear",
    "environment",
    "viewer"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.263.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^1.14.0",
    "three": "^0.159.0"
  },
  "devDependencies": {
    "@types/node": "24.2.0",
    "@types/react": "^18.0.0",
    "@types/three": "^0.150.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "public"
  ]
}